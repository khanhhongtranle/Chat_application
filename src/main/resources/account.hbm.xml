<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="Entities.Account" table="account" catalog="chatserver">
        <id name="id" type="int">
            <column name="number" />
            <generator class="identity" />
        </id>
        <property name="accountName" type="java.lang.String">
            <column length="30" name="accountname" />
        </property>
        <property name="accountPassword" type="java.lang.String">
            <column length="50" name="mypassword" />
        </property>
    </class>
</hibernate-mapping>